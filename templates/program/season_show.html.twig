{% extends "base.html.twig" %}

{% block body %}
{{dump(program, season, episodes)}}

<h1>{{program.title}} : Saison {{season.number}}</h1>
<p>{{season.description}}</p>
<h3>Année : {{season.year}}</h3>
<h3>Liste des épisodes : </h3>
<ul>
{% for episode in episodes %}
<li><a href="">{{episode.title}}</a></li>
    
{% endfor %}</ul>
{% endblock %}