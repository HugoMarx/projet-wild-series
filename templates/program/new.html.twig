{% extends "base.html.twig" %}


{% block title %}Nouvelle Série
{% endblock %}

{% block body %}

	<div class='my-5 col-6 mx-auto'>
		{{form_start(form, {'attr': {'novalidate': 'novalidate'}})}}
		{{form_widget(form)}}
		<div class="text-end"><input class='btn-secondary btn-lg' type="submit" value='Ajouter la série'></div>
		{{form_end(form)}}
		{{ form_errors(form) }}
	</div>

	<div class="text-center">
		<a href="{{path("program_index")}}">
			<button class='btn-secondary btn-lg'>Retour</button>
		</a>
	</div>
{% endblock %}
